<div class="wrapper">
    {{> navbar }}
    <h1>Friends</h1>
    <div class="container">
        <input class="search" type="text">
        <div class="friend-box-all">
            {{!-- From all nonfriends --}}
            {{#each nonfriends}} 
            {{!-- Unless they're connected/associate --}}
            {{#unless isConnected}} 
            {{!-- Their connect button will say "connect" --}}
            {{> connections connected=false}}
            {{/unless}}
            {{/each}}
        </div>
        <div class="people-box-all">
            {{!-- From all users --}}
            {{#each friends}}
            {{!-- If they're connected/already associate --}}
            {{#if isConnected}}
            {{!-- Their connect button will say "disconnect" --}}
            {{> connections connected=true}}
            {{/if}}
            {{/each}}
        </div>
    </div>
    {{> aiIcon}}
</div>