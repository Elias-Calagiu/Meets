<div class="wrapper">
    {{> navbar }}
    <div class="container row my-dflex">
        <div class="chatbox my-width-full">
            <div class="chat-content">
                <div class="my-width-full" id="history"></div>
            </div>
            <form class="my-dflex my-width-full">
                <input type="text" name="chatInput" id="input">
                <button class="btn" id="submitBtn"><i class="material-icons">send</i></button>
                <button class="btn" id="recBtn"><i class="material-icons">mic</i></button>
            </form>
        </div>
    </div>
</div>